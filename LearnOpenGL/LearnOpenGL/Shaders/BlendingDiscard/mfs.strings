#version 330 core
out     vec4    FragColor;

in     vec3    FragNormal;
in     vec3    FragPosition;
in      vec2    FragTextureCoord;

uniform sampler2D texture1;

void main ()
{
    vec4 texColor = texture(texture1, FragTextureCoord);
    if(texColor.a < 0.1)
    discard;
    FragColor = texColor;
    
}
